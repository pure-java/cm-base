<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${package.Mapper}.${table.mapperName}">
#if(${enableCache})
  <!-- 开启二级缓存 -->
  <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>
#end
#if(${baseResultMap})
  <!-- 通用查询映射结果 -->
  <resultMap id="Base_Result_Map" type="${package.Entity}.${entity}">
#foreach($field in ${table.fields})
#if(${field.keyFlag})##生成主键排在第一位
    <id column="${field.name}" property="${field.propertyName}"/>
#end
#end
#foreach($field in ${table.commonFields})##生成公共字段
    <result column="${field.name}" property="${field.propertyName}"/>
#end
#foreach($field in ${table.fields})
#if(!${field.keyFlag})##生成普通字段
    <result column="${field.name}" property="${field.propertyName}"/>
#end
#end
  </resultMap>
#end

#set ($tableName="${tableAsName}_")##表明
#set ($tableAsName="${tableName}.")##表别名
#if(${baseColumnList})
<!-- 通用查询结果列 -->
<sql id="${table.name}_base_sql">
#foreach($field in ${table.fields})
#if(${field.name} == ${field.propertyName})##判断是否是公共字段
  $tableName${field.name} as $tableAsName${field.name}#else
  $tableName${field.name} as $tableAsName${field.propertyName}#end#if(${velocityCount}<${table.fields.size()}),
#end
#end

</sql>
##${table.fieldNames}

<insert id="saveBatch">
insert into ${table.name} (
#foreach($field in ${table.fields})##生成公共字段
  ${field.name}#if($!{velocityCount}!=${table.fields.size()}),#end

#end
) values
<foreach collection="saveList" item="saveObj" separator=",">
(
#foreach($field in ${table.fields})##生成公共字段
  #{saveObj.${field.name}}#if($!{velocityCount}!=${table.fields.size()}),#end

#end
)
</foreach>
</insert>

#end
</mapper>
